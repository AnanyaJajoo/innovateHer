{
  "manifest_version": 3,
  "name": "Illume",
  "version": "1.0.1",
  "key": "",
  "description": "Detect if images on the current page are AI-generated",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "identity",
    "storage"
  ],
  "oauth2": {
    "client_id": "902516730227-m0atjinp6hf4j61tg6q1jd47hp3f2gf9.apps.googleusercontent.com",
    "scopes": ["email", "profile", "openid"]
  },
  "host_permissions": [
    "<all_urls>",
    "http://localhost:4000/*",
    "https://www.googleapis.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Illume",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
