{
  "manifest_version": 3,
  "name": "AI Image Detector",
  "version": "1.0.0",
  "description": "Detect if images on the current page are AI-generated",
  "action": {
    "default_popup": "popup.html",
    "default_title": "AI Image Detector"
  },
  "permissions": ["activeTab"],
  "host_permissions": ["<all_urls>"]
}
